sudo modprobe nvme-tcp
sudo nvme connect -t tcp -a 192.168.0.101 -s 4420 -n nvmet-export

# large test
sudo nvme io-passthru /dev/nvme0 -n 1 -o 0x21 -4 6764 -5 0 -l 6764 -i large.txt -w

# remove
sudo nvme io-passthru /dev/nvme0 -n 1 -o 0x21 -5 256

# ndp_output
sudo nvme io-passthru /dev/nvme0 -n 1 -o 0x21 -4 8224 -5 0 -l 8224 -i ndp_output -w

sudo nvme write /dev/nvme0n1 -s 0 -z 6764 -d large.txt